{"vehicle":"Passenger","quality":4,"riskRating":null,"longitude":-98.7485,"months":["Jun","Jul","Aug","Sep","Oct","Dec","Jan","Feb","Mar","Apr","May","Nov"],"latitude":20.1976,"url":"https:\/\/ropewiki.com\/Paraiso_Escondido","geojson":{"features":[{"properties":{"name":"Parking"},"type":"Feature","geometry":{"type":"Point","coordinates":[[-98.74229156575895,20.194388309062184,0]]}},{"properties":{"name":"Canyon Start"},"geometry":{"coordinates":[[-98.74826736144315,20.198119653147497,0]],"type":"Point"},"type":"Feature"},{"geometry":{"coordinates":[[-98.7482680192261,20.19958298268769,0]],"type":"Point"},"properties":{"name":"Canyon Finish"},"type":"Feature"},{"type":"Feature","properties":{"stroke":"#F0F000","name":"Approach"},"geometry":{"type":"LineString","coordinates":[[-98.74231000725558,20.194392821779203,0],[-98.74244411770636,20.19471000441531,0],[-98.74245484654242,20.194704969775337,0],[-98.74250312630468,20.194775454720155,0],[-98.74261950296886,20.194990497031135,0],[-98.74245,20.19516,0],[-98.74244634325957,20.19518072152914,0],[-98.7424387532883,20.195178225221536,0],[-98.74235292259982,20.195681686883706,0],[-98.7424333888703,20.195676652275143,0],[-98.74256749932104,20.195646444620323,0],[-98.742551,20.195642,0],[-98.742551,20.195642,0],[-98.742786,20.19575,0],[-98.742786,20.19575,0],[-98.7429,20.195835,0],[-98.7429,20.195835,0],[-98.743077,20.19583,0],[-98.743077,20.19583,0],[-98.743196,20.19571,0],[-98.743196,20.19571,0],[-98.743279,20.195706,0],[-98.743279,20.195706,0],[-98.743353,20.195705,0],[-98.743353,20.195705,0],[-98.743417,20.19569,0],[-98.743417,20.19569,0],[-98.74349,20.195698,0],[-98.74349,20.195698,0],[-98.743535,20.195685,0],[-98.743535,20.195685,0],[-98.743591,20.195667,0],[-98.743591,20.195667,0],[-98.74365,20.195659,0],[-98.74365,20.195659,0],[-98.743741,20.19561,0],[-98.743741,20.19561,0],[-98.743833,20.195568,0],[-98.743833,20.195568,0],[-98.743888,20.195558,0],[-98.743888,20.195558,0],[-98.744042,20.195552,0],[-98.744042,20.195